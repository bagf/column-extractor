{
    "name": "bagf/column-extractor",
    "require": {
        "laravel/framework": "~5.1",
        "squizlabs/php_codesniffer": "^2.3",
        "goodby/csv": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Bagf\\ColumnExtractor\\": "./src"
        }
    },
    "scripts": {
        "test": "php -l -d display_errors=0 src/ && vendor/bin/phpcs -n --standard=PSR2 --report=full --ignore=src/database/migrations/*.php src/"
    }, 
    "minimum-stability": "stable"
}
